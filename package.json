    "react-toastify": "^9.1.3",
    "redux-persist": "^6.0.0",
    "slick-carousel": "^1.8.1",
    "stripe": "^13.7.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "uuid": "^9.0.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/react-slick": "^0.23.10",
    "@types/uuid": "^9.0.2",
