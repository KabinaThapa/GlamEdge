{
  "name": "ecommerce",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "cd seeders && node seed_priceid.js",
    "server-products": "json-server -p 4001 backend/products/db.json -m ./node_modules/json-server-auth",
    "server-category": "json-server -p 4002 backend/category/db.json -m ./node_modules/json-server-auth",
    "server-subcategory": "json-server -p 4003 backend/subcategory/db.json -m ./node_modules/json-server-auth",
    "server-users": "json-server -p 4004 backend/users/db.json -m ./node_modules/json-server-auth",
    "start-all-servers": "concurrently \"npm run server-products\" \"npm run server-category\" \"npm run server-subcategory\" \"npm run server-users\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@stripe/react-stripe-js": "^2.1.2",
    "@stripe/stripe-js": "^2.1.0",
    "@types/node": "20.4.9",
    "@types/react": "18.2.19",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "concurrently": "^8.2.1",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.13",
    "formik": "^2.4.3",
    "framer-motion": "^10.15.2",
    "next": "^13.4.13",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "react-loading-skeleton": "^3.3.1",
    "react-redux": "^8.1.2",
